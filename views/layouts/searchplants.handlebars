
  <h1>Plant Search</h1>

  <!-- Search form -->
  <form id="search-form">
    <input type="text" id="search-input" placeholder="Search for any Plant">
    <button type="submit">Search</button>
  </form>
<!-- Search results -->
  <h2>Search Results</h2>
  <div id="search-results">
    {{#each plants}}
      <div class="result">
        
        <p>{{this.common_name}}</p> 
         
      </div>
    {{else}}
      <p>No results found.</p>
    {{/each}}
  </div>
  <script src="/js/plant-search.js"></script>

